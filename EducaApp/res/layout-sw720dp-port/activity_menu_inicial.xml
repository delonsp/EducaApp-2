<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_gravity="center"
    android:gravity="center"
    android:orientation="horizontal"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="@drawable/children2_port"
    tools:context=".MenuInicialActivity" >

    <ImageButton
        android:contentDescription="@string/modulos"
        android:id="@+id/btModulos"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignRight="@+id/btSuspeitar"
        android:layout_marginBottom="47dp"
        android:src="@drawable/modulos" />

    <ImageButton
        android:contentDescription="@string/suspeitar"
        android:id="@+id/btSuspeitar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="54dp"
        android:src="@drawable/suspeitar" />

    <ImageButton
        android:contentDescription="@string/dicas"
        android:id="@+id/btDicas"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:src="@drawable/dicas" />

</RelativeLayout>
