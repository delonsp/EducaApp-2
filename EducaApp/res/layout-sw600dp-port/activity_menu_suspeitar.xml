
	<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/children3_port"
   
    tools:context=".MenuModulosActivity" >

	    <TableLayout
	        android:id="@+id/btModulo1"
	        android:layout_width="@dimen/modulos_table_width"
	        android:layout_height="wrap_content"
	        android:layout_marginTop="150dp"
	        android:layout_marginLeft="80dp"
	        android:background="@color/Snow"
	        android:onClick="onClick"
	        android:orientation="horizontal" >

	        <TableRow
	            android:id="@+id/tableRow1"
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:layout_gravity="top" >

	            <ImageView
	                android:contentDescription="@string/childImage"
	                android:id="@+id/delImage"
	                android:layout_width="wrap_content"
	                android:layout_height="wrap_content"
	                android:src="@drawable/del" />
	        </TableRow>

	        <TableRow
	            android:id="@+id/tableRow2"
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content" >

	            <TextView
	                android:layout_margin="3dp"
	                android:background="@color/DarkSlateGray"
	                android:text="@string/suspeita_del_text"
	                android:textColor="@color/White"
	                android:textSize="25sp" />
	        </TableRow>

	        
	    </TableLayout>

	    <TableLayout
	        android:id="@+id/btModulo2"
	        android:layout_width="@dimen/modulos_table_width"
	        android:layout_height="wrap_content"
	        android:layout_toRightOf="@+id/btModulo1"
	        android:layout_alignTop="@+id/btModulo1"
	        android:layout_marginLeft="30dp"
	        android:background="@color/Snow"
	        android:onClick="onClick"
	        android:orientation="horizontal" >

	        <TableRow
	            android:id="@+id/tableRow6"
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:layout_gravity="top" >

	            <ImageView
	                android:contentDescription="@string/childImage"
	                android:id="@+id/autismoImage"
	                android:layout_width="wrap_content"
	                android:layout_height="wrap_content"
	                android:src="@drawable/autismo" />
	        </TableRow>

	        <TableRow
	            android:id="@+id/tableRow7"
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content" >

	            <TextView
	                android:layout_margin="3dp"
	                android:background="@color/DarkSlateGray"
	                android:text="@string/suspeita_autismo_text"
	                android:textColor="@color/White"
	                android:textSize="25sp" />
	        </TableRow>

	        

	        <TableRow
	            android:id="@+id/tableRow10"
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content" >
	        </TableRow>
	    </TableLayout>
	    
	    <TableLayout
	        android:id="@+id/btModulo3"
	        android:layout_width="@dimen/modulos_table_width"
	        android:layout_height="wrap_content"
	        android:layout_below="@+id/btModulo1"
	        android:layout_marginTop="150dp"
			android:layout_alignLeft="@+id/btModulo1"	      
	        android:background="@color/Snow"
	        android:onClick="onClick"
	        android:orientation="horizontal" >

	        <TableRow
	            android:id="@+id/tableRow11"
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:layout_gravity="top" >

	            <ImageView
	                android:contentDescription="@string/childImage"
	                android:id="@+id/dislexiaImage"
	                android:layout_width="wrap_content"
	                android:layout_height="wrap_content"
	                android:src="@drawable/dislexia" />
	        </TableRow>

	        <TableRow
	            android:id="@+id/tableRow12"
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content" >

	            <TextView
	                android:layout_margin="3dp"
	                android:background="@color/DarkSlateGray"
	                android:text="@string/suspeita_dislexia_text"
	                android:textColor="@color/White"
	                android:textSize="25sp" />
	        </TableRow>

	       
	    </TableLayout>
	    
	    
	     <TableLayout
	        android:id="@+id/btModulo4"
	        android:layout_width="@dimen/modulos_table_width"
	        android:layout_height="wrap_content"
	        android:layout_alignLeft="@+id/btModulo2"
	        android:layout_alignTop="@+id/btModulo3"
	        android:background="@color/Snow"
	        android:onClick="onClick"
	        android:orientation="horizontal" >

	        <TableRow
	            android:id="@+id/tableRow16"
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:layout_gravity="top" >

	            <ImageView
	                android:contentDescription="@string/childImage"
	                android:id="@+id/tdahImage"
	                android:layout_width="wrap_content"
	                android:layout_height="wrap_content"
	                android:src="@drawable/tdah" />
	        </TableRow>

	        <TableRow
	            android:id="@+id/tableRow17"
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content" >

	            <TextView
	                android:layout_margin="3dp"
	                android:background="@color/DarkSlateGray"
	                android:text="@string/suspeita_tdah_text"
	                android:textColor="@color/White"
	                android:textSize="25sp" />
	        </TableRow>

	        
	    </TableLayout>
	    
	    

</RelativeLayout>

